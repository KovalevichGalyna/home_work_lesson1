// Объем куба

<?php 
 $a = 6; 
 $b = $a * $a * $a;
 echo "Объем куба равен", $b, "см куб.";
 ?>		
<br>
<br>

//Длина вектора

<?php 
 $x = 4;
 $y = 5;
 $z = 6;
 $k = $x * $x + $y * $y + $z * $z; 
echo sqrt ($k),"длина данного вектора";
?> 
<br>
<br>

//Перевод км/ч в м/с

<?php 
$a = 40;
echo $a, "км/ч";
$a = $a * 1000 / 3600;
echo "=", $a, "м/с"
?>
<br>
<br>

//Возвести в 4 степень за два действия

<?php 
$a = 5;
$a = $a * $a;
$b = $a * $a;
echo $b
?>
         
